@model IEnumerable<Clinic.Database.Data.Page>

<div class="hover-dropdown">
    <ul class="navbar-nav">
        @foreach(var item in Model)
        {
            <li class="nav-item dropdown">
                <a class="nav-link" href="@item.PageLink">@item.PageAnchorText</a>
            </li>
        }
    </ul>
</div>